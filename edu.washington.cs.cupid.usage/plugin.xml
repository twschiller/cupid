<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="edu.washington.cs.cupid.preferences"
            class="edu.washington.cs.cupid.usage.preferences.UsagePreferencePage"
            id="edu.washington.cs.cupid.usage.preferences.UsagePreferencePage"
            name="Usage Data Reporting">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="edu.washington.cs.cupid.usage.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="edu.washington.cs.cupid.usage.internal.Activator"></startup>
   </extension>

</plugin>
