<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="edu.washington.cs.cupid.preferences"
            class="edu.washington.cs.cupid.conditional.preferences.FormattingPreferencePage"
            id="edu.washington.cs.cupid.conditional.preferences.FormattingPreferencePage"
            name="Conditional Formatting">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="edu.washington.cs.cupid.conditional.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="edu.washington.cs.cupid.conditional.internal.Activator">
      </startup>
   </extension>

</plugin>
